<div class="td-init">
  <i class="fa fa-bars w3-opennav w3-hide-large w3-xlarge w3-margin-left
    w3-margin-top td-menu-hide">
  </i>
  <div id="init" class="w3-container status padding block">
    <br>
    <%= image_tag "shipper.jpg", class: "td-image-shipper w3-round
      w3-animate-top" %>
    <hr>
    <div class="col-xs-12 col-sm-10 col-md-10 col-lg-10
      col-sm-offset-1 col-md-offset-1 col-lg-offset-1">
      <% @supports.init.each do |f| %>
        <div class="nht-invoice-content">
          <%= link_to [:shop, f] do %>
            <%= render "form_invoice", f: f %>
          <% end %>
          <div class="nht-btn-list-shippers">
            <%= link_to t("invoices.list_shippers"), shop_list_shippers_path(invoice_id: f.id),
              class: "btn btn-primary" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
