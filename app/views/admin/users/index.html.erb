<div class="td-manage-user col-xs-10 col-sm-10 col-md-10 col-lg-10
  col-xs-offset-2 col-sm-offset-2 col-md-offset-2 col-lg-offset-2">
  <h1 class="text-center">
    <%= t "manage_users.index.title" %>
  </h1>
  <hr>

  <table class="td-table table">
    <tr>
      <th>
        <%= t "manage_users.index.user.name" %>
      </th>
      <th>
        <%= t "manage_users.index.user.email" %>
      </th>
      <th>
        <%= t "manage_users.index.user.phone_number" %>
      </th>
      <th>
        <%= t "manage_users.index.user.role" %>
      </th>
      <th>
        <%= t "manage_users.index.user.status" %>
      </th>
      <th>
      </th>
    </tr>
    <%= render "user" %>
  </table>
  <div class="clearfix"></div>
  <%= paginate @users, theme: "twitter-bootstrap-3" %>
  <div class="tab-space"></div>
</div>
